    "@fastify/cors": "^8.5.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/session": "^10.8.0",
    "@prisma/client": "^5.7.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.2.8",
    "connect-pg-simple": "^9.0.0",
    "dotenv": "^16.3.1",
    "express-session": "^1.17.3",
    "fastify": "^4.25.2",
